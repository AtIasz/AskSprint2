@model List<Question>
@{
    ViewData["Title"] = "List of Questions";
}
<h1>@ViewData["Title"]</h1>
<h2>
    <a class="nav-link text-dark" asp-area="" asp-controller="Questions" asp-action="Add">Add A Question</a>
</h2>
<ul>
    @for (int i = 0; i < Model.Count; i++)
    {
        <li>
            <h3>
                <a class="nav-link text-dark" asp-area="" asp-controller="Questions" asp-action="Get" asp-route-id="@Model[i].ID">@Model[i].Title</a>
            </h3>

        </li>
    }


</ul>